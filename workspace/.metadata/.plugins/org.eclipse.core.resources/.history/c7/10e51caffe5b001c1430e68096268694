package ForLoop;

import java.util.Scanner;

public class Rough {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int num =n;
		int digit= num%10;
		int sum =0;
		if(digit==1) {
			sum =1;
		}
		else {sum=0;}
		num=num/10;
		int x=1;
		while(num>0) {
			digit= num%10;
			
		
			x=x*2;
			digit=digit*x;
			sum=sum+digit;
		
		num=num/10;
		}
		System.out.println(sum);
	}





}


