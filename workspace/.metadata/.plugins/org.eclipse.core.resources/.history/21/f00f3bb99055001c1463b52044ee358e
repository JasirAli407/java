package fundamentals;

public class TypeCasting {

	public static void main(String[] args) {
float a;
int b;
a =1.5f; b=1;
int c =(int)a+b;
System.out.println(c);
		
		
		//	char ch = 'a';
//	int i = ch;   // implicit conversion from small to big possible(auto)
//	ch = (char)i;  //explicit conversion bcos big to small not possible implicitly
//		 System.out.println(ch);   //in explicit typecasting there is a chance for data loss
		
	}

}
